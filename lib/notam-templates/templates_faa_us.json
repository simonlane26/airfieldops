  "templates": {
    "runwayClosure": {
      "full": "!{fdc} {icao} RWY {runway} CLSD",
      "timed": "!{fdc} {icao} RWY {runway} CLSD {startTime}-{endTime}",
      "daily": "!{fdc} {icao} RWY {runway} CLSD DLY {startTime}-{endTime}",
      "reason": "!{fdc} {icao} RWY {runway} CLSD DUE TO {reason}"
    },

    "runwayReopen": {
      "immediate": "!{fdc} {icao} RWY {runway} OPEN",
      "timed": "!{fdc} {icao} RWY {runway} OPEN EFF {time}"
    },

    "runwayReduced": {
      "length": "!{fdc} {icao} RWY {runway} {length}FT AVBL FOR TKOF AND LDG",
      "width": "!{fdc} {icao} RWY {runway} WIDTH REDUCED TO {width}FT",
      "displaced": "!{fdc} {icao} RWY {runway} THLD DSPLCD {distance}FT"
    },

    "taxiwayClosure": {
      "full": "!{fdc} {icao} TWY {taxiway} CLSD",
      "timed": "!{fdc} {icao} TWY {taxiway} CLSD {startTime}-{endTime}",
      "section": "!{fdc} {icao} TWY {taxiway} BTN {pointA} AND {pointB} CLSD",
      "reason": "!{fdc} {icao} TWY {taxiway} CLSD DUE TO {reason}"
    },

    "taxiwayReopen": {
      "immediate": "!{fdc} {icao} TWY {taxiway} OPEN",
      "timed": "!{fdc} {icao} TWY {taxiway} OPEN EFF {time}"
    },

    "rampClosure": {
      "full": "!{fdc} {icao} RAMP {ramp} CLSD",
      "partial": "!{fdc} {icao} RAMP {ramp} SPOTS {spots} CLSD",
      "timed": "!{fdc} {icao} RAMP {ramp} CLSD {startTime}-{endTime}",
      "reason": "!{fdc} {icao} RAMP {ramp} CLSD DUE TO {reason}"
    },

    "construction": {
      "runway": "!{fdc} {icao} RWY {runway} CONST {description}",
      "taxiway": "!{fdc} {icao} TWY {taxiway} CONST {description}",
